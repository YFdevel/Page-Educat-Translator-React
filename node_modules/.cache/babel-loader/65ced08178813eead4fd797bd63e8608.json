F), \\u0442\\u0430\\u043A \\u0438 \\u0431\\u044D\\u043A\\u0435\\u043D\\u0434 (\\u0445\\u0440\\u0430\\u043D\\u0435\\u043D\\u0438\\u0435 \\u0438 \\u043E\\u0431\\u0440\\u0430\\u0431\\u043E\\u0442\\u043A\\u0430 \\u0434\\u0430\\u043D\\u043D\\u044B\\u0445, \\u043D\\u0430\\u043F\\u0440\\u0438\\u043C\\u0435\\u0440 \\u0432\\u0430\\u0448\\u0438 \\u043B\\u0430\\u0439\\u043A\\u0438 \\u0438 \\u043A\\u043E\\u043C\\u043C\\u0435\\u043D\\u0442\\u0430\\u0440\\u0438\\u0438 \\u0432 \\u0438\\u043D\\u0441\\u0442\\u0430\\u0433\\u0440\\u0430\\u043C).\"})]}),/*#__PURE__*/_jsxs(\"div\",{children:[/*#__PURE__*/_jsx(\"p\",{children:/*#__PURE__*/_jsx(\"img\",{src:image3})}),/*#__PURE__*/_jsx(\"p\",{className:\"headline\",children:\"IT \\u0441\\u043F\\u0435\\u0446\\u0438\\u0430\\u043B\\u0438\\u0441\\u0442\\u0430\\u043C\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u0412\\u043E\\u0437\\u043C\\u043E\\u0436\\u043D\\u043E \\u0432\\u044B \\u0443\\u0436\\u0435 \\u043D\\u0430\\u0448\\u0438 \\u043A\\u043E\\u043B\\u043B\\u0435\\u0433\\u0438, \\u043D\\u043E \\u0438\\u0437 \\u0434\\u0440\\u0443\\u0433\\u043E\\u0439 \\u0441\\u0444\\u0435\\u0440\\u044B \\u0440\\u0430\\u0437\\u0440\\u0430\\u0431\\u043E\\u0442\\u043A\\u0438. \\u042D\\u0442\\u043E\\u0442 \\u043A\\u0443\\u0440\\u0441 \\u043F\\u043E\\u0437\\u0432\\u043E\\u043B\\u0438\\u0442 \\u0432\\u0430\\u043C \\u0438\\u0437\\u0443\\u0447\\u0438\\u0442\\u044C \\u0441\\u043E\\u0432\\u0440\\u0435\\u043C\\u0435\\u043D\\u043D\\u044B\\u0435 \\u0444\\u0440\\u0435\\u0439\\u043C\\u0432\\u043E\\u0440\\u043A\\u0438 \\u0438 \\u043C\\u0435\\u0442\\u043E\\u0434\\u044B \\u0440\\u0430\\u0437\\u0440\\u0430\\u0431\\u043E\\u0442\\u043A\\u0438 \\u043A\\u043B\\u0438\\u0435\\u043D\\u0442-\\u0441\\u0435\\u0440\\u0432\\u0435\\u0440\\u043D\\u044B\\u0445 \\u0432\\u0435\\u0431 \\u043F\\u0440\\u0438\\u043B\\u043E\\u0436\\u0435\\u043D\\u0438\\u0439.\"})]})]})]});};export default Home;","map":{"version":3,"sources":["D:/Educat courses/Практика_Educat/React JS Создание страницы educat.courses/Проект/my-app-1/src/Views/Pages/Home/index.js"],"names":["React","Video","image1","image2","image3","Home"],"mappings":"AAAA,MAAOA,CAAAA,KAAP,KAAkB,OAAlB,CACA,MAAOC,CAAAA,KAAP,KAAkB,uBAAlB,CACA,MAAOC,CAAAA,MAAP,KAAmB,wCAAnB,CACA,MAAOC,CAAAA,MAAP,KAAmB,wCAAnB,CACA,MAAOC,CAAAA,MAAP,KAAmB,wCAAnB,C,wFAGA,GAAMC,CAAAA,IAAI,CAAC,QAALA,CAAAA,IAAK,EAAI,CACX,mBACI,aAAK,SAAS,CAAC,WAAf,wBACI,aAAK,SAAS,CAAC,MAAf,wBAAsB,kHAAtB,cACI,gGADJ,cAEI,oGAFJ,cAGI,iJAHJ,cAII,qGAJJ,GADJ,cAMI,aAAK,SAAS,CAAC,WAAf,wBACI,oCACI,UAAG,SAAS,CAAC,UAAb,0JADJ,cAEI,6qBAFJ,GADJ,cAMI,oCACI,UAAG,SAAS,CAAC,UAAb,gCADJ,cAEI,8lCAFJ,GANJ,cAaI,oCACI,UAAG,SAAS,CAAC,UAAb,iKADJ,cAEI,s/BAFJ,GAbJ,GANJ,cA0BI,KAAC,KAAD,IA1BJ,cA2BI,UAAG,EAAE,CAAC,SAAN,kFA3BJ,cA4BI,aAAK,SAAS,CAAC,QAAf,wBACI,oCACI,gCAAG,YAAK,GAAG,CAAEH,MAAV,EAAH,EADJ,cAEI,UAAG,SAAS,CAAC,UAAb,qOAFJ,cAGI,kzBAHJ,GADJ,cAOI,oCACI,gCAAG,YAAK,GAAG,CAAEC,MAAV,EAAH,EADJ,cAEI,UAAG,SAAS,CAAC,UAAb,yJAFJ,cAGI,8lCAHJ,GAPJ,cAeI,oCACI,gCAAG,YAAK,GAAG,CAAEC,MAAV,EAAH,EADJ,cAEI,UAAG,SAAS,CAAC,UAAb,yFAFJ,cAGI,+3BAHJ,GAfJ,GA5BJ,GADJ,CAuDH,CAxDD,CAyDA,cAAgBC,CAAAA,IAAhB","sourcesContent":["import React from \"react\";\r\nimport Video from \"./Components/Video.js\";\r\nimport image1 from \"../../../../public/Images/Group 30.png\";\r\nimport image2 from \"../../../../public/Images/Group 27.png\";\r\nimport image3 from \"../../../../public/Images/Group 31.png\";\r\n\r\n\r\nconst Home=()=>{\r\n    return(\r\n        <div className=\"home-page\">\r\n            <div className=\"info\"><span>Цена курса: 4 650 ₽/мес</span>\r\n                <span>Старт: 1 апреля</span>\r\n                <span>Формат: Онлайн</span>\r\n                <span>Длительность: 12 месяцев</span>\r\n                <span>Уровень: с нуля</span></div>\r\n            <div className=\"textBlock\">\r\n                <div>\r\n                    <p className=\"headline\">Разработчик полного цикла</p>\r\n                    <p>Full stack — это разработчик полного цикла, который может создать из прототипа\r\n                        полноценный продукт(сайт, интернет-магазин, онлайн-школа).</p>\r\n                </div>\r\n                <div>\r\n                    <p className=\"headline\">Frontend + Backend</p>\r\n                    <p>Full stack-разработчик владеет как фронтенд (все то, что видит пользователь на экране, например\r\n                        внешний\r\n                        вид сайта или веб-приложения), так и бэкенд\r\n                        (хранение и обработка данных, например ваши лайки и комментарии в инстаграм).</p>\r\n                </div>\r\n                <div>\r\n                    <p className=\"headline\">Для новичков и специалистов</p>\r\n                    <p>Наш курс Full stack — разработчик, подходит как новичкам, с нулевым знанием в этой области, так и\r\n                        специалистам, которые хотят\r\n                        прокачать свои знания и обладать конкурентным преимуществом на бирже труда.</p>\r\n                </div>\r\n            </div>\r\n            <Video/>\r\n            <p id=\"forWhat\">Для кого курс</p>\r\n            <div className=\"blocks\">\r\n                <div>\r\n                    <p><img src={image1}/></p>\r\n                    <p className=\"headline\">Желающим быстро войти в сферу разработки</p>\r\n                    <p>Уже были попытки обучения в сфере web разработки, но не хватило мотивации? У нас скучно не будет.\r\n                        Вы начнете делать свой реальный проект с первого дня обучения.</p>\r\n                </div>\r\n                <div>\r\n                    <p><img src={image2}/></p>\r\n                    <p className=\"headline\">Начинающим разработчикам</p>\r\n                    <p>Full stack-разработчик владеет как фронтенд (все то, что видит пользователь на экране, например\r\n                        внешний\r\n                        вид сайта или веб-приложения), так и бэкенд\r\n                        (хранение и обработка данных, например ваши лайки и комментарии в инстаграм).</p>\r\n                </div>\r\n                <div>\r\n                    <p><img src={image3}/></p>\r\n                    <p className=\"headline\">IT специалистам</p>\r\n                    <p>Возможно вы уже наши коллеги, но из другой сферы разработки. Этот курс позволит вам изучить\r\n                        современные\r\n                        фреймворки\r\n                        и методы разработки клиент-серверных веб приложений.</p>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\nexport  default Home;"]},"metadata":{},"sourceType":"module"}